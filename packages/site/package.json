{
  "name": "site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "yarn@3.6.3",
  "scripts": {
    "allow-scripts": "yarn workspace root allow-scripts",
    "start": "dotenv -e ../../.env vite",
    "build": "tsc && dotenv -e ../../.env vite build",
    "preview": "dotenv -e ../../.env vite preview",
    "test": "dotenv -e ../../.env vite test",
    "format:check": "prettier -c \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint:ts:check": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:css:check": "stylelint \"src/**/*.{css,scss,sass}\"",
    "lint:check": "yarn lint:ts:check && yarn lint:css:check",
    "lint": "yarn lint:ts:check --fix && yarn lint:css:check --fix",
    "ts:check": "tsc --noEmit",
    "check": "yarn format && yarn format:check && yarn lint:check && yarn ts:check"
  },
  "dependencies": {
    "@csstools/normalize.css": "^12.0.0",
    "@reduxjs/toolkit": "^1.9.7",
    "bootstrap": "^5.1.3",
    "classnames": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^8.1.3",
    "react-responsive": "^9.0.0-beta.6",
    "react-router": "^6.18.0",
    "react-router-dom": "^6.18.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "@vitejs/plugin-react": "^4.0.3",
    "babel-eslint": "^10.1.0",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.7.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.5.1",
    "sass": "^1.54.0",
    "stylelint": "^14.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-declaration-strict-value": "^1.8.0",
    "stylelint-no-px": "^1.0.1",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.1.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.5"
  }
}
